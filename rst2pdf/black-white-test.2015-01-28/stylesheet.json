        "width": "11in",
        "height": "8.5in",
        "margin-top": "2cm",
        "margin-bottom": "2cm",
        "margin-left": "1cm",
        "margin-right": "1cm",
        "spacing-header": "1cm",
        "spacing-footer": "1cm"
        
    },

    "pageTemplates" : {
